{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12, 
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    
    "rules": {
        "max-len": [2, 120, 4]
    }
}
